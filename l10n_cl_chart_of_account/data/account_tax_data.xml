    <record id="IVAV_19" model="account.tax.template">
      <field name="chart_template_id" ref="cl_chart_template_sii"/>
      <field name="name">IVA 19% Venta</field>
      <field name="description">IVA 19% Venta</field>
      <field name="amount">19</field>
      <field name="sii_code">14</field>
      <field name="amount_type">percent</field>
      <field name="type_tax_use">sale</field>
      <field name="sequence" eval="1"/>
      <field name="invoice_repartition_line_ids" eval="[(5, 0, 0),
          (0,0, {
              'factor_percent': 100,
              'repartition_type': 'base',
              'plus_report_expression_ids': [ref('tax_report_ventas_netas_gravadas_c_iva')],
          }),
          (0,0, {
              'factor_percent': 100,
              'repartition_type': 'tax',
              'account_id': ref('210701'),
              'plus_report_expression_ids': [ref('tax_report_iva_debito_fiscal')],
          }),
      ]"/>
      <field name="refund_repartition_line_ids" eval="[(5, 0, 0),
          (0,0, {
              'factor_percent': 100,
              'repartition_type': 'base',
              'minus_report_expression_ids': [ref('tax_report_ventas_netas_gravadas_c_iva')],
          }),
          (0,0, {
              'factor_percent': 100,
              'repartition_type': 'tax',
              'account_id': ref('210701'),
              'minus_report_expression_ids': [ref('tax_report_iva_debito_fiscal')],
