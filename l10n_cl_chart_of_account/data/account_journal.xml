<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<!--Account Journal -->
	<record id="fee_journal" model="account.journal">
		<field name="name">Honorarios</field>
		<field name="code">BHO</field>
		<field name="type">purchase</field>
		<field name="show_on_dashboard" eval="True" />
	</record>

	<record id="pcga_journal" model="account.journal">
		<field name="name">Sólo PCGA</field>
		<field name="code">PCGA</field>
		<field name="type">general</field>
		<field name="show_on_dashboard" eval="False" />
	</record>

	<record id="ifrs_journal" model="account.journal">
		<field name="name">Sólo IFRS</field>
		<field name="code">IFRS</field>
		<field name="type">general</field>
		<field name="show_on_dashboard" eval="False" />
	</record>

</odoo>						
